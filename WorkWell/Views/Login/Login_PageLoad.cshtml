@model WorkWell.Models.LoginCls
@{
    ViewBag.Title = "Login_PageLoad";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Please Login</h2>
<div>
    @using(Html.BeginForm("Login_click", "Login", FormMethod.Get))
    {
        <div>
            <label>Username:</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.Uname)
            @Html.ValidationMessageFor(m => m.Uname)
        </div>
        <div>
            <label>Password:</label>
        </div>
        <div>
            @Html.PasswordFor(m => m.pass)
            @Html.ValidationMessageFor(m => m.pass)
        </div>

        <div>
            <input type="submit" value="Login" />
            @Html.DisplayFor(m=>m.msg)
        </div>

    }
</div>

